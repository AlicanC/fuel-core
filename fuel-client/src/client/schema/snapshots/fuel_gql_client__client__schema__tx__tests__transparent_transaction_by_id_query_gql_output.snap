---
source: fuel-client/src/client/schema/tx.rs
assertion_line: 264
expression: operation.query

---
query Query($_0: HexString256!) {
  transaction(id: $_0) {
    gasLimit
    gasPrice
    bytePrice
    id
    inputColors
    inputContracts
    inputs {
      __typename
      ... on InputCoin {
        utxoId
        owner {
          address
        }
        amount
        color
        witnessIndex
        maturity
        predicate
        predicateData
      }
      ... on InputContract {
        utxoId
        balanceRoot
        stateRoot
        contractId
      }
    }
    isScript
    outputs {
      __typename
      ... on CoinOutput {
        to {
          address
        }
        amount
        color
      }
      ... on ContractOutput {
        inputIndex
        balanceRoot
        stateRoot
      }
      ... on WithdrawalOutput {
        to {
          address
        }
        amount
        color
      }
      ... on ChangeOutput {
        to {
          address
        }
        amount
        color
      }
      ... on VariableOutput {
        to {
          address
        }
        amount
        color
      }
      ... on ContractCreated {
        contractId
        stateRoot
      }
    }
    maturity
    receiptsRoot
    status {
      __typename
      ... on SubmittedStatus {
        time
      }
      ... on SuccessStatus {
        blockId
        time
        programState {
          returnType
          data
        }
      }
      ... on FailureStatus {
        blockId
        time
        reason
        programState {
          returnType
          data
        }
      }
    }
    witnesses
    receipts {
      a
      b
      amount
      color
      gas
      digest
      id
      is
      pc
      ptr
      ra
      rb
      rc
      rd
      reason
      receiptType
      to
      toAddress {
        address
      }
      val
      len
      result
      gasUsed
      data
    }
    script
    scriptData
    salt
    staticContracts
    storageSlots
    bytecodeWitnessIndex
  }
}

