---
source: fuel-client/src/client/schema/block.rs
assertion_line: 84
expression: operation.query

---
query Query($_0: HexString256) {
  block(id: $_0) {
    height
    id
    time
    producer {
      address
    }
    transactions {
      rawPayload
      receipts {
        rawPayload
      }
      status {
        __typename
        ... on SubmittedStatus {
          time
        }
        ... on SuccessStatus {
          blockId
          time
          programState {
            returnType
            data
          }
        }
        ... on FailureStatus {
          blockId
          time
          reason
          programState {
            returnType
            data
          }
        }
      }
    }
  }
}

